{
  "name": "@claims/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:db:push": "dotenv -e .env.test -- prisma db push",
    "test:db:reset": "dotenv -e .env.test -- prisma migrate reset --force",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.943.0",
    "@aws-sdk/s3-request-presigner": "^3.943.0",
    "@claims/shared": "*",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "better-auth": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "hashids": "^2.3.0",
    "helmet": "^8.1.0",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "pg-boss": "^12.5.2",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "resend": "^6.5.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.14",
    "dotenv-cli": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.0.1",
    "supertest": "^7.1.4",
    "tsx": "^4.19.2",
    "vitest": "^4.0.14"
  }
}
